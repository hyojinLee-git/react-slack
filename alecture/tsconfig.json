{
    "compilerOptions": {
        "esModuleInterop": true,//import * as React from 'react';->import React from 'react';
        "sourceMap": true,
        "lib":["ES2020","DOM"],
        "jsx":"react",
        "module": "esnext",
        "moduleResolution": "Node",
        "target":"es5",
        "strict": true,
        "resolveJsonModule": true,
        "baseUrl": ".",
        "paths": {  //  import A from ./../../../hello.js->import A from @src/hello.js
            "@hooks/*":["hooks/*"],
            "@components/*":["components/*"],
            "@layouts/*":["layouts/*"],
            "@pages/*":["pages/*"],
            "@utils/*":["utils/*"],
            "@typings/*":["typings/*"]
        }

    }
}